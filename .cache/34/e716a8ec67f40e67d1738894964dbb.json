  return {\r\n          loggIn: this.$store.getters.IS_LOGG_IN,\r\n          log: this.$store.getters.GET_USER_NAME\r\n        }\r\n      },\r\n    },\r\n    data(){\r\n      return {\r\n        RModal: false,\r\n        AModal: false,\r\n      }\r\n    },\r\n    methods: {\r\n      AuthOpen(){\r\n          this.RModal = false,\r\n          this.AModal = true\r\n      },\r\n      AuthClose(){\r\n        this.AModal = false\r\n      },\r\n      RegOpen(){\r\n          this.RModal = true,\r\n          this.AModal = false\r\n      },\r\n      RegClose(){\r\n        this.RModal = false\r\n      },\r\n      LogOut(){\r\n        this.$store.dispatch('LOGOUT');\r\n      }\r\n    }\r\n  };\r\n\r\n</script>\r\n\r\n<style></style>\r\n"},"lineCount":null}},"error":null,"hash":"23d8da11187e12a632d169fde1ed7e3b","cacheData":{"env":{}}}