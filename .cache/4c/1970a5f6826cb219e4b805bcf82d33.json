ource":"pages/singlepage/index.vue","name":null,"original":{"line":76,"column":0},"generated":{"line":87,"column":18}},{"source":"pages/singlepage/index.vue","name":null,"original":{"line":77,"column":0},"generated":{"line":88,"column":0}},{"source":"pages/singlepage/index.vue","name":null,"original":{"line":33,"column":0},"generated":{"line":89,"column":0}}],"sources":{"pages/singlepage/index.vue":"<template>\r\n  <div class=\"singlepage\">\r\n    <h1>{{dataList.article.head}} </h1>\r\n    <div id='text' ></div>\r\n\r\n    <div class='comment_head'>\r\n      <h2>Комментарии:</h2>\r\n    </div>\r\n    <div v-for='value in dataList.comments'>\r\n      <div class='comment_print'>\r\n        <div class='head_comment'>\r\n          <div class='login'><b>{{value.login}}</b></div>\r\n          <div class='date'>\r\n            <i>\r\n              {{EditDate(value.date)}}\r\n            </i>\r\n          </div>\r\n        </div>\r\n        <div class='text_comment'>{{value.text}}</div>\r\n      </div>\r\n    </div>\r\n    <div v-if='!dataList.loggIn'><b><i>!!!Чтобы оставить комментарий, пожалуйста, авторизуйтесь на сайте!!!</i></b></div>\r\n    <a v-if='dataList.loggIn' @click='add_comment'>Добавить комментарий</a>\r\n    <CommentForm v-if=\"comment\"> </CommentForm>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport CommentForm from \"../../components/CommentForm/index.vue\";\r\n\r\nexport default {\r\n  name: \"SinglePage\",\r\n  components: {\r\n    CommentForm,\r\n  },\r\n  computed: {\r\n    dataList() {\r\n      return {\r\n        article: this.$store.getters.GET_ARTICLE,\r\n        comments: this.$store.getters.GET_COMMENTS,\r\n        loggIn: this.$store.getters.IS_LOGG_IN,\r\n      }\r\n    }\r\n  },\r\n  data(){\r\n    return {\r\n      comment: false,\r\n    }\r\n  },\r\n  methods: {\r\n    PasteText(){\r\n      let d = document.getElementById('text');\r\n      d.insertAdjacentHTML('beforeend', this.$store.getters.GET_ARTICLE.text);\r\n    },\r\n    add_comment(){\r\n      this.comment = !this.comment\r\n    },\r\n    EditDate(date){\r\n      date = new Date(date);\r\n      let minutes = date.getMinutes();\r\n      let hours = date.getHours();\r\n\r\n      if (0<=minutes && minutes<=9){\r\n        minutes = '0' + String(date.getMinutes());\r\n      };\r\n      if (0<=hours && hours<=9){\r\n        hours = '0' + String(date.getHours());\r\n      };\r\n\r\n      return `${date.getFullYear()}/${date.getMonth()+1}/${date.getDate()}  ${hours}:${minutes}`;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.PasteText();\r\n  }\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"},"lineCount":null}},"error":null,"hash":"796bb0dd186fc4a67a3f3433976694ea","cacheData":{"env":{}}}